<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/linearLayout2"
    android:layout_width="199dp"
    android:layout_height="69dp"
    android:orientation="horizontal">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:clickable="true"
        android:backgroundTint="@color/green_V4"
        android:contentDescription="@string/todo"
        android:src="@drawable/round_account_balance_24"
        app:tint="@color/black" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/go_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:onClick="volverAlHome"
        android:contentDescription="@string/new_message"
        android:src="@drawable/outline_arrow_upward_24"
        app:tint="@color/black"
        android:backgroundTint="@color/green_V4"
        />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/add_note"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:onClick="OpenActivityTareas"
        android:src="@drawable/baseline_note_add_24"
        app:tint="@color/black"
        android:backgroundTint="@color/green_V4"
        />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="cerrarSesion"
        android:src="@drawable/ic_logout"
        app:tint="@color/black"
        android:backgroundTint="@color/green_V4"

        />

</LinearLayout>
